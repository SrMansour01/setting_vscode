{
    "workbench.colorTheme": "Soudev Mega Dark",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorCustomizations": {
        "editorBracketMatch.border": "#404040",
        "editorLineNumber.foreground": "#4c1d95",
        "editorLineNumber.activeForeground": "#4b5563",
        "errorForeground": "#7f1d1d",
        "minimap.background": "#070E13",
        "minimap.selectionHighlight": "#404040",
        "minimap.errorHighlight": "#7f1d1d",
        "minimap.warningHighlight": "#fb923c",
        "terminal.foreground": "#404040",
        "terminalCursor.foreground": "#4b5563"
    },
    "editor.minimap.size": "proportional",
    "editor.autoClosingBrackets": "always",
    "editor.fontSize": 14,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontWeight": "100",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 1,
    "terminal.integrated.fontFamily": "Fira Code",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontWeight": "100",
}
